  },
  "ava": {
    "require": [
      "./tests/js/helpers/setup-browser-env.js"
    ],
    "files": [
      "tests/js/**/*.js"
    ],
    "source": [
      "sickchill/gui/slick/js/**/*.{js,jsx}",
      "!dist/**/*",
      "!lib/**/*",
      "!**/*.min.js"
    ]
  },
  "xo": {
    "space": 4,
    "rules": {
      "unicorn/filename-case": "off",
      "unicorn/prefer-node-append": "off"
    },
    "esnext": true,
    "envs": [
      "browser"
    ],
    "globals": [
      "_",
      "scRoot",
      "jQuery",
      "$",
      "metaToBool",
      "getMeta",
      "PNotify",
      "themeSpinner",
      "anonURL",
      "Gettext",
      "gt",
      "_n",
      "latinize"
    ],
    "ignores": [
      "core.min.js",
      "vendor.min.js",
      "lib/**/*",
      "Gruntfile.js",
      "sickchill/gui/slick/js/lib/*"
    ]
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
